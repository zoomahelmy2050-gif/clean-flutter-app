# Production Backend Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

POSTGRES_HOST=your-postgres-host
POSTGRES_PORT=5432
POSTGRES_DATABASE=security_app_db
POSTGRES_USERNAME=your-username
POSTGRES_PASSWORD=your-password

# Real-time Communication
WEBSOCKET_URL=wss://your-websocket-server.com
SOCKET_IO_URL=https://your-socketio-server.com
WEBSOCKET_AUTH_TOKEN=your-websocket-auth-token

# External Threat Intelligence APIs
VIRUSTOTAL_API_KEY=your-virustotal-api-key
ALIENVAULT_API_KEY=your-alienvault-api-key
SHODAN_API_KEY=your-shodan-api-key
ABUSEIPDB_API_KEY=your-abuseipdb-api-key

# Email and SMS Services
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=security@yourcompany.com
SENDGRID_FROM_NAME=Security Team

TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Cloud Storage Configuration
# AWS S3
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=security-evidence-storage

# Google Cloud Storage
GCP_PROJECT_ID=your-gcp-project-id
GCP_SERVICE_ACCOUNT_KEY=path/to/service-account-key.json
GCS_BUCKET_NAME=security-evidence-gcs

# Azure Blob Storage
AZURE_STORAGE_ACCOUNT=your-storage-account
AZURE_STORAGE_KEY=your-storage-key
AZURE_CONTAINER_NAME=security-evidence

# Cryptographic Services
# Hardware Security Module (HSM)
HSM_ENDPOINT=https://your-hsm-endpoint.com
HSM_API_KEY=your-hsm-api-key

# Key Management Service (KMS)
AWS_KMS_KEY_ID=your-aws-kms-key-id
AZURE_KEY_VAULT_URL=https://your-vault.vault.azure.net/
GCP_KMS_KEY_RING=your-gcp-key-ring

# Compliance Platform APIs
# ServiceNow
SERVICENOW_INSTANCE=your-instance.service-now.com
SERVICENOW_API_KEY=your-servicenow-api-key

# RSA Archer
ARCHER_INSTANCE=https://your-archer-instance.com
ARCHER_API_KEY=your-archer-api-key

# MetricStream
METRICSTREAM_API_KEY=your-metricstream-api-key

# Resolver
RESOLVER_API_KEY=your-resolver-api-key

# LogicGate
LOGICGATE_API_KEY=your-logicgate-api-key

# AuditBoard
AUDITBOARD_API_KEY=your-auditboard-api-key

# MDM Provider APIs
# Microsoft Intune
INTUNE_TENANT_ID=your-tenant-id
INTUNE_CLIENT_ID=your-client-id
INTUNE_CLIENT_SECRET=your-client-secret
INTUNE_ACCESS_TOKEN=your-access-token

# VMware Workspace ONE
WORKSPACE_ONE_API_KEY=your-workspace-one-api-key
WORKSPACE_ONE_TENANT_CODE=your-tenant-code
WORKSPACE_ONE_INSTANCE=your-instance.awmdm.com

# JAMF
JAMF_INSTANCE=your-instance.jamfcloud.com
JAMF_API_TOKEN=your-jamf-api-token

# MobileIron
MOBILEIRON_INSTANCE=your-instance.mobileiron.com
MOBILEIRON_API_KEY=your-mobileiron-api-key

# Citrix Endpoint Management
CITRIX_INSTANCE=your-instance.cloud.com
CITRIX_API_KEY=your-citrix-api-key

# BlackBerry UEM
BLACKBERRY_INSTANCE=your-instance.blackberry.com
BLACKBERRY_API_KEY=your-blackberry-api-key

# Security Configuration
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key
API_RATE_LIMIT=1000
SESSION_TIMEOUT=3600

# Monitoring and Analytics
SENTRY_DSN=your-sentry-dsn
FIREBASE_PROJECT_ID=your-firebase-project-id
ANALYTICS_API_KEY=your-analytics-api-key

# Development/Testing
DEBUG_MODE=false
LOG_LEVEL=info
MOCK_EXTERNAL_APIS=false
